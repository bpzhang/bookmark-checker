  },
  "deleting": {
    "message": "Deleting...",
    "description": "Text shown when deleting"
  },
  "openLink": {
    "message": "Open Link",
    "description": "Text for open link action"
  },
  "delete": {
    "message": "Delete",
    "description": "Text for delete action"
  },
  "status": {
    "message": "Status: $STATUS$",
    "placeholders": {
      "status": {
        "content": "$1"
      }
    },
    "description": "Text for status display"
