    "description": "Text shown when checking a specific bookmark"
  },
  "checkComplete": {
    "message": "Check complete!\nValid: $VALID$\nInvalid: $INVALID$\nExcluded: $EXCLUDED$",
    "placeholders": {
      "valid": {
        "content": "$1"
      },
      "invalid": {
        "content": "$2"
      },
      "excluded": {
        "content": "$3"
      }
    },
    "description": "Text shown when check is complete"
  },
  "invalidBookmarksList": {
    "message": "Invalid Bookmarks List",
    "description": "Title for invalid bookmarks list"
  },
  "deleteAllInvalid": {
    "message": "Delete All Invalid",
