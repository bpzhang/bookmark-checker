  },
  "checked": {
    "message": "Checked",
    "description": "Text for checked count"
  },
  "valid": {
    "message": "Valid",
    "description": "Text for valid bookmarks"
  },
  "invalid": {
    "message": "Invalid",
    "description": "Text for invalid bookmarks"
  },
  "excluded": {
    "message": "Excluded",
    "description": "Text for excluded bookmarks"
  },
  "readyToCheck": {
    "message": "Ready to check...",
    "description": "Text shown before checking starts"
  },
  "checkingBookmark": {
    "message": "Checking: $BOOKMARK$",
